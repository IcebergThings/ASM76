# Search target
DATB	0x4 $4
# Search index
DATB	0x0 $8
# Array start point
DATI	[ARRAY] $12
# Array pointer: $16
# Loop inc
DATB	0x1 $24

[loop]
MVRB	$12 $16
ADDI	$16 $8
LDBR	$16 $20
CMPB	$20 $4
JIEA	[end]
ADDB	$8 $24
JMPA	[loop]

[end]
SLIA	0x100 $8
INTX	0x4 0x100
HALT

# Data after HALT
[ARRAY]
RAWD	0x0001 0x0003 0x0002 0x0005 0x0004
RAWD	0x0006 0x0008 0x0007 0x000A 0x0009

